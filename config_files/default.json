{
	"servers":
	{
		"listen":				"8080",
		"host":					"127.0.0.1",
		"server_names":			"localhost",
		"client_max_body_size":	"1M",
		"error_pages":			{"404":"html/PageNotFound.html"},
		"auto_index":			"false",
		"cgi_extents":			".py",
		"cgi_paths":			"/usr/bin/python3",
		"locations":
		[
			{
				"match":			"/",
				"allowed_methods":	["GET", "DELETE"],
				"root":				"html/",
				"index":			"home.html"
			},
			{
				"match":			"/cgi-bin",
				"allowed_methods":	"POST",
				"root":				"cgi-bin/",
				"index":			"upload.py"
			}
		]
	}
}
